{
    "name": "ouun/laravel-tailwind-config",
    "description": "Get access to the tailwind config values in your Laravel Project",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Boffey",
            "email": "michael@approved.io"
        },
        {
            "name": "Philipp Wellmer",
            "email": "philipp@wellmer.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "6.8.*"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Stage\\LaravelTailwindConfig\\": "src/"
        }
    },
    "extra": {
        "acorn": {
            "providers": [
                "Stage\\LaravelTailwindConfig\\LaravelTailwindConfigServiceProvider"
            ],
            "aliases": {
                "Tailwind": "Stage\\LaravelTailwindConfig\\Facades\\Tailwind"
            }
        }
    }
}
